{"ast":null,"code":"import LoginNavBar from \"./LoginNavBar.vue\";\nimport NavFeature from \"./NavFeature.vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"RemoveCert\",\n  components: {\n    LoginNavBar,\n    NavFeature\n  },\n\n  data() {\n    return {\n      rollNo: \"\"\n    };\n  },\n\n  methods: {\n    async delCert() {\n      let result = await axios.delete(`http://localhost:4000/certs/certs/${this.rollNo}`);\n\n      if (result.status == 200) {\n        alert(\"Certificate Deleted\");\n      }\n\n      localStorage.setItem(\"user-info\", JSON.stringify(result.data));\n      this.$router.push({\n        name: \"AddCert\"\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAaA,OAAOA,WAAP,MAAwB,mBAAxB;AACA,OAAOC,UAAP,MAAuB,kBAAvB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;AACbC,MAAI,EAAE,YADO;AAEbC,YAAU,EAAE;AACVJ,eADU;AAEVC;AAFU,GAFC;;AAMbI,MAAI,GAAG;AACL,WAAO;AACLC,YAAM,EAAE;AADH,KAAP;AAGD,GAVY;;AAWbC,SAAO,EAAE;AACP,UAAMC,OAAN,GAAgB;AACd,UAAIC,MAAK,GAAI,MAAMP,KAAK,CAACQ,MAAN,CAChB,qCAAoC,KAAKJ,MAAO,EADhC,CAAnB;;AAGA,UAAIG,MAAM,CAACE,MAAP,IAAiB,GAArB,EAA0B;AACxBC,aAAK,CAAC,qBAAD,CAAL;AACF;;AACAC,kBAAY,CAACC,OAAb,CAAqB,WAArB,EAAkCC,IAAI,CAACC,SAAL,CAAeP,MAAM,CAACJ,IAAtB,CAAlC;AACA,WAAKY,OAAL,CAAaC,IAAb,CAAkB;AAAEf,YAAI,EAAE;AAAR,OAAlB;AACD;;AAVM;AAXI,CAAf","names":["LoginNavBar","NavFeature","axios","name","components","data","rollNo","methods","delCert","result","delete","status","alert","localStorage","setItem","JSON","stringify","$router","push"],"sourceRoot":"","sources":["C:\\Users\\Priyank\\OneDrive\\Desktop\\publi_verify_cert-master\\frontend\\src\\components\\RemoveCert.vue"],"sourcesContent":["<template>\n  <div class=\"page\">\n    <LoginNavBar />\n    <div class=\"certificate\">\n      <div id=\"fr\"><NavFeature /></div>\n      <div class=\"frm\">\n        <div><input type=\"text\" placeholder=\"Roll No\" v-model=\"rollNo\" /></div>\n        <div><button v-on:click=\"delCert()\">Delete</button></div>\n      </div>\n    </div>\n  